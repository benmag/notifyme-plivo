{
    "name": "benmag/notifyme-plivo",
    "version": "1.0.0",
    "description": "Provides a plivo gateway for NotifyMe",
    "license": "MIT",
    "keywords": ["notifymehq", "notifyme", "plivo"],
    "authors": [
        {
            "name": "Ben M",
            "homepage": "https://github.com/benmag"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "~5.3",
        "notifymehq/notifyme": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotifyMeHQ\\Plivo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotifyMeHQ\\Plivo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
